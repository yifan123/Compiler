	const int j = 100;
	int sum,i,array[100];
	sum = 0;
	i = 0;
	while (i < j) {
		array[i] = i;
		i = i + 1;
	}
	;;;;;;
	{{{}}{}}
	i = 0;
	while (i < j) {
		sum = sum + i;
		i = i + 1;
	}
